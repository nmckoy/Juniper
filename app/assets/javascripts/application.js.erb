<% 
  vendors = ['jquery',
              'jquery_ujs',
              'bootstrap.min',
              'respond.min',
              'html5shiv',
              'usebootstrap',
              'grayscale',
              'jquery.easing.min',
              'angular',
              'angular-route',
              'angular-resource',
              'angular-rails-templates']
              
  internal = [
              '../javascripts/ng-app/config',
              '/app/assets/javascripts/ng-app/*.js',
          	  '/app/assets/javascripts/ng-app/modules/**/*js',
          	  '/app/assets/javascripts/ng-app/modules/**/**/*js'
          	 ]
%>
<% vendors.each do |dep| 
    require_asset "#{dep}.js"
  end
%>

<% internal.each do |dep|
    require_asset "#{dep}"
  end 
%>


